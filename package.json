{
  "name": "magnetic-clouds",
  "version": "1.0.0",
  "description": "Magnetic Clouds - Premium Web Hosting & Domain Provider from Bangladesh",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "db:migrate": "node server/database/migrate.js",
    "db:seed": "node server/database/seed.js"
  },
  "keywords": [
    "hosting",
    "domain",
    "cloud",
    "vps",
    "bangladesh"
  ],
  "author": "Magnetic Clouds",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.0",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mailgun.js": "^10.4.0",
    "mariadb": "^3.2.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "slugify": "^1.6.6",
    "stripe": "^20.0.0",
    "uuid": "^9.0.1"
  },
  "optionalDependencies": {
    "sharp": "^0.33.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
